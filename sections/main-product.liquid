{{ 'section-main-product.css' | asset_url | stylesheet_tag }}
<link rel="stylesheet" href="{{ 'section-main-product-desktop.css' | asset_url }}" media="screen and (min-width: 768px)">
{% render 'prepare-style', file: 'component-accordion.css' %}
{{ 'component-price.css' | asset_url | stylesheet_tag }}
{{ 'component-slider.css' | asset_url | stylesheet_tag }}
{{ 'component-loading-overlay.css' | asset_url | stylesheet_tag }}

{% render 'prepare-style', file: 'component-pickup-availability.css' %}
