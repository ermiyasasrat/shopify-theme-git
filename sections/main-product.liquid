										</option>
									{%- endif -%}
								{%- endfor -%}
								</select>
							</div>
							</div>
						{%- endfor -%}

						<script type="application/json">
							{{ product.variants | json }}
						</script>
						</variant-selects>
					{%- endif -%}
					{%- endunless -%}
				{%- endif -%}
