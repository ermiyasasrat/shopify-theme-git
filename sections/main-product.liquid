    padding-top: {{ section.settings.padding_top | times: 0.75 | round: 0 }}px;
    padding-bottom: {{ section.settings.padding_bottom | times: 0.75 | round: 0 }}px;
  }

  @media screen and (min-width: 750px) {
    .section-{{ section.id }}-padding {
      padding-top: {{ section.settings.padding_top }}px;
      padding-bottom: {{ section.settings.padding_bottom }}px;
    }
  }
{%- endstyle -%}
{% comment %}{%- if product.media.size > 0 -%}
  <script src="{{ 'product-media-gallery.js' | asset_url }}" defer="defer"></script>
{%- endif -%}{% endcomment %}
<script>
    document.addEventListener('DOMContentLoaded', function() {
        // $('#rapid_reviews_items').closest('.shopify-app-block').addClass('app-section-reviews');
        $('.trustpilot-wd-large-review').closest('.shopify-section').addClass('app-section-reviews');
    });
</script>
<script>
    document.addEventListener('DOMContentLoaded', function() {
        resourceManager.loadResourceOnInteractive([
            { "name": "script", "url": "{{ 'product-form.js' | asset_url }}" },
            { "name": "script", "url": "{{ 'product-upsells.js' | asset_url }}" },
            { "name": "script", "url": "{{ 'product-quantity-input.js' | asset_url }}" },
            { "name": "script", "url": "{{ 'product-ie-input.js' | asset_url }}" },
            { "name": "script", "url": "https://cdn.roomvo.com/static/scripts/b2b/storiesflooringcouk.js" },
            { "name": "script", "url": "{{ 'product-pickup-availability.js' | asset_url }}" },
        ], {
            triggerEvents: {
                readyInteractive: true,
                elementsIntersection: 'product-quantity-input, variant-selects, pickup-availability'
            }
        });

        resourceManager.loadResourceOnInteractive([
            { "name": "script", "url": "{{ 'product-media-gallery.js' | asset_url }}" },
        ], {
            triggerEvents: {
                readyInteractive: true,
                elementsIntersection: 'media-gallery'
            }
        });
    });
</script>
{% assign popup_products = product.metafields.shopify--discovery--product_recommendation.complementary_products.value %}
{%- assign first_3d_model = product.media | where: "media_type", "model" | first -%}
{%- if first_3d_model -%}
  {{ 'component-product-model.css' | asset_url | stylesheet_tag }}
  <link id="ModelViewerStyle" rel="stylesheet" href="https://cdn.shopify.com/shopifycloud/model-viewer-ui/assets/v1.0/model-viewer-ui.css" media="print" onload="this.media='all'">
  <link id="ModelViewerOverride" rel="stylesheet" href="{{ 'component-model-viewer-ui.css' | asset_url }}" media="print" onload="this.media='all'">
{%- endif -%}
<section class="site-wrap section-{{ section.id }}-padding">
  <div id="productTemplate-{{ section.id }}" class="product product--{{ section.settings.media_size }} product--{{ section.settings.gallery_layout }} grid grid--1-col grid--2-col-tablet">
	{% comment %}
		Get selected variant and sample variant
	{% endcomment %}
	{%- assign sampleVariantId = "" -%}
	{%- assign sampleVariantTitle = "" -%}
	{%- assign selectedVariant = product.selected_or_first_available_variant -%}

	{%- assign current_variant = selectedVariant -%}
	{%- assign productType = product.type | downcase | replace: ' ', '_' | slice: 0, 20 -%}
	{%- assign productUpsells = product.metafields.product_upsells.product | split: '|' -%}
	{%- assign productRelated = product.metafields.related_products.product | split: '|' -%}

	{% comment %}
		Product metafields
	{% endcomment %}

	{%- if product.variants.size > 1 -%}
		{%- assign productPrice = current_variant.price | money_without_currency -%}
		{%- assign productComparePrice = current_variant.compare_at_price | money_without_currency -%}
		{%- assign productComparePricePerMetresSquared = current_variant.metafields.product_config.compare_price_per_metres_sq -%}
		{%- assign productWarranty = current_variant.metafields.product_attribute.warranty -%}
	{%- else -%}
		{%- assign productPrice = product.price | money_without_currency -%}
		{%- assign productComparePrice = product.compare_at_price | money_without_currency -%}
		{%- assign productComparePricePerMetresSquared = product.metafields.product_config.compare_price_per_metres_sq -%}
		{%- assign productWarranty = product.metafields.product_attribute.warranty -%}
	{%- endif -%}

	{%- assign productNumber = current_variant.metafields.product_config.product_number -%}
	{%- assign productWidth = current_variant.metafields.product_attribute.width -%}
	{%- assign productMinQty = current_variant.metafields.product_config.minimum_qty -%}
	{%- assign productRrp = current_variant.metafields.filters.rrp | plus: 0 -%}
	{%- assign productPackSize = current_variant.metafields.product_attribute.pack_size -%}

	{%- assign productRollWidth = current_variant.metafields[productType].roll_width -%}
	{%- if productRollWidth == blank and current_variant.metafields.product_config.sale_metric == "Roll" -%}
		{%- assign productRollWidth = current_variant.metafields.product_attribute.width -%}
	{%- endif -%}

	{%- if current_variant.metafields.product_config.sale_metric == "Pack" -%}
		{%- assign productPricePerMetresSquared = productPrice | divided_by: productPackSize | round: 2 -%}
		{%- assign squareMetrePence = productPricePerMetresSquared | times: 100 -%}
		{%- assign packPrice = productPrice -%}
	{%- endif -%}

	{%- assign productLength = current_variant.metafields.filters.length -%}
	{%- assign piecesPerPack = product.metafields.filters.pieces_per_pack -%}

	{%- if current_variant.metafields.product_config.sale_metric == "Box" -%}
		{%- assign wasPrice = current_variant.compare_at_price | divided_by: 100 -%}
	{%- else -%}
		{%- if product.variants.size == 1 -%}
			{%- assign wasPrice = product.compare_at_price | divided_by: productPackSize | divided_by: 100 -%}
		{%- elsif current_variant.compare_at_price == blank -%}
			{%- assign wasPrice = product.compare_at_price | divided_by: productPackSize | divided_by: 100 -%}
		{%- else -%}
			{%- assign wasPrice = current_variant.compare_at_price | divided_by: productPackSize | divided_by: 100 -%}
		{%- endif -%}
	{%- endif -%}

  {%- assign discontinued_product = product.metafields.product_config.discontinued_product | downcase -%}
  {%- assign offline_product = product.metafields.product_config.offline_product | downcase -%}

	{% comment %}
		Get discounted price and percentage change
	{% endcomment %}

	{%- if wasPrice != blank or wasPrice != 0 -%}
		{%- assign denominator = wasPrice -%}
	{%- endif -%}

	{%- if current_variant.metafields.product_config.sale_metric == "Box" or current_variant.metafields.product_config.sale_metric == "Tile" -%}
		{%- assign numerator = productPrice | replace:',','' -%}
	{%- else -%}
		{%- assign numerator = productPricePerMetresSquared | replace:',','' -%}
	{%- endif -%}

	{%- assign newPriceMinusOld = numerator | minus: denominator -%}
	{%- assign differenceDividedByOld = newPriceMinusOld | divided_by: denominator -%}
	{%- assign differencePercentage = differenceDividedByOld | times: -100 -%}
	{%- assign formatPercentage = differencePercentage | round: 0 -%}

	{% comment %}
		Check if there's only two variants because
		one is the free sample variant
	{% endcomment %}
	{%- assign onlySampleAndProduct = false -%}

	{% comment %}
		Check if there's an even pack restriction
	{% endcomment %}
	{%- assign evenPackRestriction = false -%}
	{%- for tag in product.tags -%}
		{%- if tag == "EvenPackRestriction" -%}
			{%- assign evenPackRestriction = true -%}
		{%- endif -%}
	{%- endfor -%}

	{%- if squareMetrePence == blank or squareMetrePence == 0 -%}
		{%- assign checkedPencePrice = current_variant.price -%}
	{%- else -%}
		{%- assign checkedPencePrice = squareMetrePence -%}
	{%- endif -%}
	{%- if productPricePerMetresSquared == blank or productPricePerMetresSquared == 0 -%}
		{%- assign checkedSquareMetrePrice = current_variant.price -%}
	{%- else -%}
		{%- assign checkedSquareMetrePrice = productPricePerMetresSquared -%}
	{%- endif -%}

	{%- if productRollWidth != blank and productPricePerMetresSquared == blank and current_variant.metafields.product_config.sale_metric == "Roll" -%}
		{%- assign checkedSquareMetrePrice = current_variant.price | divided_by: productRollWidth | times: 0.01 -%}
		{%- assign checkedPencePrice = current_variant.price -%}
	{%- elsif current_variant.metafields.product_config.sale_metric == "Box" or current_variant.metafields.product_config.sale_metric == "Tile" -%}
		{%- assign checkedSquareMetrePrice = current_variant.price | times: 0.01 -%}
		{%- assign checkedPencePrice = current_variant.price -%}
	{%- endif -%}

	{%- assign variantTitle = current_variant.title -%}

	{% comment %}
		Check for accessory restriction
	{% endcomment %}
  {%- assign isAccessoryProduct = false -%}
  {%- assign isLussoProduct = false -%}
  {%- assign isLussoAccessoryProduct = false -%}

  {%- for collection in product.collections -%}
    {%- if collection.title contains 'Accessories' -%}
      {%- assign isAccessoryProduct = true -%}
    {%- endif -%}
    {%- if collection.title contains 'Lusso' -%}
      {%- assign isLussoProduct = true -%}
    {%- endif -%}
  {%- endfor -%}

  {% if isAccessoryProduct and isLussoProduct %}
    {%- assign isLussoAccessoryProduct = true -%}
  {% endif %}

  {%- assign accessoryRestriction = false -%}
  {%- assign lussoAccessoryRestriction = false -%}

  {%- assign hasFlooringItem = false -%}
  {%- assign hasAccessoryItem = false -%}
  {%- assign hasLussoFlooringItem = false -%}
  {%- assign hasLussoAccessoryItem = false -%}

  {%- if cart.item_count > 0 -%}
    {%- for item in cart.items -%}
      {%- assign isAccessoryItem = false -%}
      {%- assign isLussoItem = false -%}

      {%- for collection in item.product.collections -%}
        {%- if collection.title contains 'Accessories' -%}
          {%- assign hasAccessoryItem = true -%}
          {%- assign isAccessoryItem = true -%}
        {%- endif -%}
        {%- if collection.title contains 'Lusso'-%}
          {%- assign isLussoItem = true -%}
        {%- endif -%}
      {%- endfor -%}
      {%- if isAccessoryItem and isLussoItem -%}
        {%- assign hasLussoAccessoryItem = true -%}
      {%- endif -%}

      {% unless isAccessoryItem %}
        {% if item.final_line_price > 0 %}
          {%- assign hasFlooringItem = true -%}
          {%- if isLussoItem -%}
            {%- assign hasLussoFlooringItem = true -%}
          {%- endif -%}
        {% endif %}
      {% endunless %}
    {%- endfor -%}
  {%- endif -%}

  {% if isAccessoryProduct %}
    {% unless hasFlooringItem %}
      {%- assign accessoryRestriction = true -%}
    {% endunless %}
  {% endif %}
  {% if isLussoAccessoryProduct %}
    {% unless hasLussoFlooringItem %}
      {%- assign accessoryRestriction = true -%}
      {%- assign lussoAccessoryRestriction = true -%}
    {% endunless %}
  {% endif %}

  {%- assign deliveryDate = product.metafields.product-delivery.text | date: '%s' -%}
  {%- assign variantDeliveryDate = current_variant.metafields.product-delivery.text | date: '%s' -%}
  {%- assign todaysDate = "now" | date: "%s" -%}

    <div class="grid__item product__media-wrapper">
      {% include 'product-media-gallery' %}
    </div>
    <div class="product__info-wrapper grid__item{% if settings.page_width > 1400 and section.settings.media_size == "small" %} product__info-wrapper--extra-padding{% endif %}">
      <div id="ProductInfo-{{ section.id }}" class="product__info-container{% if section.settings.enable_sticky_info %} product__info-container--sticky{% endif %}">
        {% render 'breadcrumbs' class: 'dark' static: true %}
        {%- assign product_form_id = 'product-form-' | append: section.id -%}

        {%- for block in section.blocks -%}
          {%- case block.type -%}
          {%- when '@app' -%}
            {% render block %}
          {%- when 'text' -%}
            <p class="product__text{% if block.settings.text_style == 'uppercase' %} caption-with-letter-spacing{% elsif block.settings.text_style == 'subtitle' %} subtitle{% endif %}" {{ block.shopify_attributes }}>
              {{- block.settings.text -}}
            </p>
          {%- when 'title' -%}
            <h1 class="product__title" {{ block.shopify_attributes }}>
              {{ product.title | escape }}
			</h1>
          <div class="stock-status">
            <!-- {% if product.available %}
              {% if product.variants.size > 1 %}
                {% for variant in product.variants %}
                  <p>
                    {% if variant.inventory_quantity > 0 %}
                      <span class="in-stock">In Stock</span>
                    {% else %}
                      <span class="out-stock">Out of Stock</span>
                    {% endif %}
                  </p>
                {% endfor %}
              {% else %}
                {% if product.variants.first.inventory_quantity > 0 %}
                  <span class="in-stock">In Stock</span>
                {% else %}
                  <span class="out-stock">Out of Stock</span>
                {% endif %}
              {% endif %}
            {% else %}
              <span style="color: red;">Out of Stock</span>
            {% endif %} -->
            {%- if product.available == false -%}
              <span class="out-stock">Out of Stock</span>
            {%- else -%}
              <span class="in-stock">In Stock</span>
            {% endif %}
          </div>
          {%- when 'trustpilot_wd_mini' -%}
              {% render 'trustpilot-wd-prod-mini', product_sku: current_variant.sku %}
          {%- when 'product_collections' -%}
              {% render 'product-collections' %}
          {%- when 'reviews' -%}
            {% render 'judgeme_widgets', widget_type: 'judgeme_preview_badge', jm_style: '', concierge_install: true, product: product %}
          {% when 'product-number' %}
            {% if productNumber %}
                    <div class="product__number" data-visual-data="product_number">
                        {{ productNumber }}
                    </div>
          {% endif %}

		  {%- when 'next-day-delivery' -%}
			{% if product.tags contains "Delivery_NextDay" %}
				<div class="product-delivery-banner-container">
					<div class="product-delivery-banner-inner-container">
						<div class="delivery-banner-lorry-outer-container">
							<div class="delivery-banner-lorry-container">
								<img
									src="{{ 'lorry.svg' | asset_url }}"
									class="lorry-svg"
									alt="Lorry"
								/>
							</div>
						</div>
						<div class="delivery-banner-text-container">
							<h4>
								{{ 'products.delivery.title' | t }}
							</h4>
							<p>
								{{ 'products.delivery.description' | t }}
							</p>
						</div>
					</div>
				</div>
			{% endif %}
		  {%- when 'price' -%}
			{%- comment -%}
				{% render 'product-ld-json'
					productPricePerMetresSquared: checkedSquareMetrePrice,
					squareMetrePence: checkedPencePrice,
					productRrp: productRrp
				%}
			{%- endcomment -%}
			{%- if product.metafields.product_attribute.show_quote_form == true -%}
				<div class="klaviyo-form-XfbH4X"></div>
			{%- elsif discontinued_product == "true" -%}
				{% render 'recommended-products'
					snippetHeading: "This product is discontinued unfortunately."
				%}
			{%- elsif offline_product == "true" -%}
				{% include 'product-offline' %}
			{%- elsif product.available == false -%}
			{%- else -%}
				{% render 'recommended-product-modal' %}
				<div class="no-js-hidden product-price" id="price-{{ section.id }}" role="status" {{ block.shopify_attributes }}>
					{%- render 'price',
						product: product,
						selectedVariant: selectedVariant
						use_variant: true,
						show_badges: true,
						price_class: 'price--large',
						productMinQty: productMinQty,
						evenPackRestriction: evenPackRestriction,
						sampleVariantId: sampleVariantId,
						productPackSize: productPackSize,
						current_variant: current_variant,
						productPricePerMetresSquared: checkedSquareMetrePrice,
						squareMetrePence: checkedPencePrice,
						productRrp: productRrp,
						formatPercentage: formatPercentage
					-%}
				</div>

				{%- if product.metafields.product-delivery.text != blank and deliveryDate > todaysDate -%}
					<div class="future-delivery-date-container">
						<p>
							{{ 'products.delivery.later_delivery' | t }} {{ product.metafields.product-delivery.text }}
						</p>
					</div>
				{%- elsif current_variant.metafields.product-delivery.text != blank and variantDeliveryDate > todaysDate -%}
					<div class="future-delivery-date-container">
						<p>
							{{ 'products.delivery.later_delivery' | t }} {{ current_variant.metafields.product-delivery.text }}
						</p>
					</div>
				{%- endif -%}
				<div {{ block.shopify_attributes }}>
				{%- form 'product', product, id: 'product-form-installment', class: 'installment caption-large' -%}
					<input type="hidden" name="id" value="{{ product.selected_or_first_available_variant.id }}">
					{{ form | payment_terms }}
				{%- endform -%}
				</div>

				{%- if current_variant.metafields.product_config.sale_metric == "Board" -%}
					{%- if productLength != blank and productWidth != blank -%}
						{%- assign boardPricePerMetre = selectedVariant.price | divided_by: productPackSize -%}
						<span class="board-metre-price">
							{{ boardPricePerMetre | money }}/mÂ²
						</span>
					{%- endif -%}
				{%- endif -%}
			{%- endif -%}

      {%- when 'description' -%}
        {%- if product.description != blank -%}
          <div class="product__description rte">
            {{ product.description }}
          </div>
        {%- endif -%}

      {%- when 'custom_liquid' -%}
        {{ block.settings.custom_liquid }}
      {%- when 'collapsible_tab' -%}
        <div class="product__accordion accordion" {{ block.shopify_attributes }}>
          <details id="Details-{{ block.id }}-{{ section.id }}">
            <summary>
              <div class="summary__title">
                {% render 'icon-accordion', icon: block.settings.icon %}
                <h2 class="h4 accordion__title">
                  {{ block.settings.heading | default: block.settings.page.title }}
                </h2>
              </div>
              {% render 'icon-caret' %}
            </summary>

            <div class="accordion__content rte" id="ProductAccordion-{{ block.id }}-{{ section.id }}">
              {{ block.settings.content }}
              {{ block.settings.page.content }}
            </div>
          </details>
        </div>
		  {%- when 'quantity_selector' -%}
		  	{%- if product.metafields.product_attribute.show_quote_form == true -%}
		 	{%- elsif offline_product == "true" -%}
			{%- elsif discontinued_product == "true" -%}
			{%- elsif product.available == false -%}
				{%- render 'product-out-of-stock' -%}
			{%- else -%}
				{%- assign boardSize = selectedVariant.title | divided_by: 100 -%}
				{% render 'product-quantity'
					selectedVariant: selectedVariant
					productMinQty: productMinQty,
					evenPackRestriction: evenPackRestriction,
					sampleVariantId: sampleVariantId,
					productPackSize: productPackSize,
					current_variant: current_variant,
					productPricePerMetresSquared: checkedSquareMetrePrice,
					squareMetrePence: checkedPencePrice,
					productRrp: productRrp
					type: current_variant.metafields.product_config.sale_metric,
					productRollWidth: productRollWidth,
					accessoryRestriction: accessoryRestriction,
					boardSize: productPackSize,
					packPrice: packPrice,
					variantTitle: variantTitle,
					width: productWidth,
					length: productLength,
					piecesPerPack: piecesPerPack
				%}
			{%- endif -%}
          {%- when 'popup' -%}
            <modal-opener class="product-popup-modal__opener no-js-hidden" data-modal="#PopupModal-{{ block.id }}" {{ block.shopify_attributes }}>
              <button id="ProductPopup-{{ block.id }}" class="product-popup-modal__button link" type="button" aria-haspopup="dialog">{{ block.settings.text | default: block.settings.page.title }}</button>
            </modal-opener>
            <a href="{{ block.settings.page.url }}" class="product-popup-modal__button link no-js">{{ block.settings.text }}</a>
          {%- when 'share' -%}
            <share-button id="Share-{{ section.id }}" class="share-button" {{ block.shopify_attributes }}>
              <button class="share-button__button hidden">
                {% render 'icon-share' %}
                {{ block.settings.share_label | escape }}
              </button>
              <details id="Details-{{ block.id }}-{{ section.id }}">
                <summary class="share-button__button">
                  {% render 'icon-share' %}
                  {{ block.settings.share_label | escape }}
                </summary>
                <div id="Product-share-{{ section.id }}" class="share-button__fallback motion-reduce">
                  <div class="field">
                    <span id="ShareMessage-{{ section.id }}" class="share-button__message hidden" role="status">
                    </span>
                    <input type="text"
                          class="field__input"
                          id="url"
                          value="{{ product.selected_or_first_available_variant.url | default: product.url | prepend: request.origin }}"
                          placeholder="{{ 'general.share.share_url' | t }}"
                          onclick="this.select();"
                          readonly
                    >
                    <label class="field__label" for="url">{{ 'general.share.share_url' | t }}</label>
                  </div>
                  <button class="share-button__close hidden no-js-hidden">
                    {% render 'icon-close' %}
                    <span class="visually-hidden">{{ 'general.share.close' | t }}</span>
                  </button>
                  <button class="share-button__copy no-js-hidden">
                    {% render 'icon-clipboard' %}
                    <span class="visually-hidden">{{ 'general.share.copy_to_clipboard' | t }}</span>
                  </button>
                </div>
              </details>
            </share-button>
            <script src="{{ 'share.js' | asset_url }}" defer="defer"></script>
		  {%- when 'variant_picker' -%}
		  {%- if offline_product == "true" -%}
      {%- elsif discontinued_product == "true" -%}
			{%- elsif product.available == false -%}
			{%- else -%}
				{%- if onlySampleAndProduct == false -%}
					{%- unless product.has_only_default_variant -%}
					{%- if block.settings.picker_type == 'button' -%}
						<variant-radios class="no-js-hidden" data-section="{{ section.id }}" data-url="{{ product.url }}" {{ block.shopify_attributes }}>
						{%- for option in product.options_with_values -%}
							<fieldset class="js product-form__input">
								<legend class="form__label">{{ option.name }}</legend>
								{%- for value in option.values -%}
								<input type="radio" id="{{ section.id }}-{{ option.position }}-{{ forloop.index0 }}"
										name="{{ option.name }}"
										value="{{ value | escape }}"
										form="{{ product_form_id }}"
										{% if option.selected_value == value %}checked{% endif %}
								>
