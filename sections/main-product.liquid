{% render 'prepare-style', file: 'component-trustpilot-review.css' %}
<link rel="preload" as="script" href="{{ 'product-form.js' | asset_url }}">
<link rel="preload" as="script" href="{{ 'product-upsells.js' | asset_url }}">
<link rel="preload" as="script" href="{{ 'product-quantity-input.js' | asset_url }}">
<link rel="preload" as="script" href="{{ 'product-ie-input.js' | asset_url }}">
